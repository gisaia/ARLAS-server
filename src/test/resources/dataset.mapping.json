{
  "_all": {
    "enabled": true
  },
  "_source": {
    "enabled": true
  },
  "dynamic": true,
  "properties": {
    "id": {
      "type": "keyword"
    },
    "fullname": {
      "type": "text"
    },
    "params": {
      "properties": {
        "age": {
          "type": "integer"
        },
        "job": {
          "type": "keyword"
        },
        "startdate": {
          "type": "date",
          "format": "epoch_millis"
        }
      }
    },
    "geo_params": {
      "properties": {
        "geometry": {
          "type": "geo_shape"
        },
        "centroid": {
          "type": "geo_point"
        }
      }
    }
  }
}
