      <xs:element name="LayerMinTileRow" type="xs:integer"/>
      <xs:element name="LayerMaxTileRow" type="xs:integer"/>
      <xs:element name="LayerMinTileCol" type="xs:integer"/>
      <xs:element name="LayerMaxTileCol" type="xs:integer"/>
    </xs:sequence>
  </xs:complexType>
  <!--
 RS	Coordinate reference systems and Geographical grid systems		TWG-RS	I
 GN	Geographical names							TWG-GN	I
 AU	Administrative units							TWG-AU	I
 AD	Addresses								TWG-AD	I
 CP	Cadastral parcels							TWG-CP	I
 TN	Transport networks							TWG-TN	I
 HY	Hydrography								TWG-HY	I
 PS	Protected sites								TWG-PS	I
 
 EL	Elevation								TWG-EL	II
 LC	Land cover								TWG-LC	II
 OR	Orthoimagery								TWG-OR	II
 GE	Geology									TWG-GE	II
 
 SU	Statistical units							TWG-SU	III
 BU	Buildings								TWG-BU	III
 SO	Soil									TWG-SO	III
 LU	Land use								TWG-LU	III
 HH	Human health and safety							TWG-HH	III
 UT	Utility and governmental services					TWG-UT	III
 EF	Environmental monitoring facilities					TWG-EF	III
 PF	Production and industrial facilities					TWG-PF	III
 AF	Agricultural and aquacultural facilities				TWG-AF	III
 PD	Population distribution, demography					TWG-PD	III
 AM	Area management/restriction/regulation zones and reporting units	TWG-AM	III
 NR	Natural risk zones							TWG-NR	III
 AC	Atmospheric conditions							TWG-AC	III
 MF	Meteorological geographical features					TWG-MF	III
 OF	Oceanographic geographical features					TWG-OF	III
 SR	Sea regions								TWG-SR	III
 BR	Bio-geographical regions						TWG-BR	III
 HB	Habitats and biotopes							TWG-HB	III
 SD	Species distribution							TWG-SD	III
-->
  <xs:simpleType name="harmonisedLayerName">
    <xs:restriction base="xs:string">
      <xs:pattern value="(RS|GN|AU|AD|CP|TN|HY|PS)\..+"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="harmonisedLayerName_GN">
    <xs:union>
      <xs:simpleType>
        <xs:restriction base="harmonisedLayerName">
          <xs:enumeration value="GN.GeographicalNames"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>
  <xs:simpleType name="harmonisedLayerName_AU">
    <xs:union>
      <xs:simpleType>
        <xs:restriction base="harmonisedLayerName">
          <xs:enumeration value="AU.AdministrativeUnit"/>
          <xs:enumeration value="AU.AdministrativeBoundary"/>
          <xs:enumeration value="AU.Condominium"/>
          <xs:enumeration value="AU.Baseline"/>
          <xs:enumeration value="AU.MaritimeBoundary"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="harmonisedLayerName">
          <xs:pattern value="(AU)\..+"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>
  <xs:simpleType name="harmonisedLayerName_AD">
    <xs:union>
      <xs:simpleType>
        <xs:restriction base="harmonisedLayerName">
          <xs:enumeration value="AD.Address"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>
  <xs:simpleType name="harmonisedLayerTitle">
    <xs:restriction base="notEmptyString">
      <!--			<xs:pattern value="(RS|GN|AU|AD|CP|TN|HY|PS)\..+"/>-->
    </xs:restriction>
